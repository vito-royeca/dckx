platform :ios, '13.0'
use_frameworks!

def shared_pods
  use_frameworks!
  pod 'FontAwesome.swift'
  pod 'MBProgressHUD'
  pod 'PromiseKit'
  pod 'SDWebImageSwiftUI', '>= 1.0.0-beta2'
  pod 'Sync'
end

target 'dckx' do
    shared_pods
end

target 'dckxTests' do
  shared_pods
end

target 'dckxUITests' do
  shared_pods
end
